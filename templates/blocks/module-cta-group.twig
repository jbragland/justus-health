{% extends 'blocks/module.twig' %}

{% block content %}

	{% if module.heading %}
		<h2 class="cta-group__heading">{{ module.heading }}</h2>
	{% endif %}

	{% if module.cta_group|length > 0 %}
		<div class="row">
			{% for cta in module.cta_group %}
				<div class="col-md-6 col-lg-3 cta-group__col">
					{% include 'blocks/cta-group-cta.twig' %}
				</div>
			{% endfor %}
		</div>
	{% endif %}

{% endblock %}
